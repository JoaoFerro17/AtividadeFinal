programa {
  funcao inicio() {
    inteiro n1, n2, soma, sub
    escreva("Digite um número qualquer \n")
    leia(n1)
    escreva("Digite outro número qualquer \n")
    leia(n2)

    soma = n1 + n2 
    sub = n1 - n2
    escreva("A soma de ", n1, " + ", n2, "é igual a: ", soma)
    escreva("A subtração de ", n1, " - ", n2, "é igual a: ", sub)

  }
}
